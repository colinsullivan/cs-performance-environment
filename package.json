{
  "name": "cs-performance-environment-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.8.0",
    "@serialport/bindings": "^9.0.1",
    "@supercollider/lang": "^1.0.1",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tonaljs/tonal": "^4.6.0",
    "axios": "^0.21.1",
    "bjorklund-js": "^0.1.2",
    "bootstrap": "^4.5.2",
    "csstype": "^3.0.11",
    "d3-scale": "^4.0.2",
    "dotenv": "^16.0.0",
    "easymidi": "^1.1.2",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "inobounce": "^0.1.6",
    "isomorphic-ws": "^4.0.1",
    "jss": "^10.9.0",
    "lodash": "^4.17.20",
    "nexusui": "^2.1.6",
    "node-osc": "^3.0.1",
    "note-parser": "^2.0.1",
    "pex-color": "^1.1.1",
    "pixi.js": "^4.8.9",
    "pluralize": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.9.0",
    "react-nexusui": "^0.1.7",
    "react-redux": "^7.2.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass": "^1.32.13",
    "serialport": "^9.2.0",
    "styled-components": "^5.2.1",
    "supercollider-redux": "1.0.0-alpha.3",
    "supercollider-redux-sequencers": "0.9.13",
    "typescript": "^4.1.2",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1",
    "ws": "^8.5.0"
  },
  "main": "build/server/index.js",
  "scripts": {
    "start_react": "react-scripts start",
    "build_react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start_server_dev": "npm run build_server && NODE_ENV=development node ./build/main.js",
    "start_server": "node ./build/main.js",
    "build_server": "mkdir -p build/main && TS_NODE_PROJECT=server.tsconfig.json ts-node ./scripts/build.ts",
    "build": "npm run build_react && npm run build_server",
    "build_docs": "plantuml -progress -tpng ./docs/*.puml",
    "postinstall": "npm run build",
    "read_serial": "TS_NODE_PROJECT=server.tsconfig.json node -r ts-node/register -r tsconfig-paths/register scripts/readSerial.ts",
    "serialports:list": "TS_NODE_PROJECT=server.tsconfig.json node -r ts-node/register -r tsconfig-paths/register scripts/listSerialDevices.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "block-scoped-var": "error",
      "no-case-declarations": 0,
      "prefer-const": "error",
      "react/prop-types": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "@typescript-eslint/ban-ts-comment": "off"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.20.47",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/serialport": "^8.0.1",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "esbuild": "^0.14.28",
    "fs-extra": "^10.0.1",
    "prettier": "^2.1.2",
    "redux-mock-store": "^1.5.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
