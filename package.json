{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@serialport/bindings": "^9.0.1",
    "axios": "^0.18.1",
    "bjorklund-js": "^0.1.2",
    "bootstrap": "^4.5.2",
    "easymidi": "^1.1.2",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "hammerjs": "^2.0.8",
    "inobounce": "^0.1.6",
    "lodash": "^4.17.20",
    "node-osc": "^3.0.1",
    "note-parser": "^2.0.1",
    "pex-color": "^1.1.1",
    "pixi.js": "^4.8.9",
    "pluralize": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hammerjs": "^1.0.1",
    "react-redux": "^7.2.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0",
    "supercollider-redux": "file:../supercollider-redux",
    "supercollider-redux-sequencers": "0.9.0",
    "uuid": "^3.4.0"
  },
  "main": "build/server/index.js",
  "scripts": {
    "start_react": "react-scripts start",
    "build_react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start_electron": "NODE_ENV=development electron -r @babel/register ./src/main/index.js",
    "rebuild_electron": "electron-rebuild",
    "postinstall": "npm run dev_codesign",
    "dev_codesign": "codesign --force --deep --sign - node_modules/electron/dist/Electron.app",
    "build_electron": "babel --out-dir ./build/main/ src/main/*.js && babel --out-dir ./build/common/ src/common/*.js",
    "start": "electron build/main/",
    "build": "npm run build_react && npm run build_electron",
    "build_docs": "plantuml -progress -tpng ./docs/*.puml",
    "package": "electron-packager . cs-performance-environment --platform darwin --overwrite"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error",
      "no-case-declarations": 0,
      "prefer-const": "error",
      "react/prop-types": 0
    }
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-plugin-istanbul": "^5.2.0",
    "electron": "^6.1.12",
    "electron-packager": "^13.1.1",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^1.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
