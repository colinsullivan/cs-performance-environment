{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "bjorklund-js": "^0.1.2",
    "bootstrap": "^4.1.3",
    "easymidi": "0.0.4",
    "eslint": "^6.6.0",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "hammerjs": "^2.0.8",
    "inobounce": "^0.1.6",
    "lodash": "^4.17.11",
    "node-osc": "^3.0.0",
    "note-parser": "^2.0.1",
    "pex-color": "^1.1.1",
    "pixi.js": "^4.8.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hammerjs": "^1.0.1",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
    "supercollider-redux": "file:../supercollider-redux",
    "supercollider-redux-sequencers": "file:../supercollider-redux-sequencers",
    "uuid": "^3.3.2"
  },
  "main": "build/server/index.js",
  "scripts": {
    "start_react": "react-scripts start",
    "build_react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start_electron": "NODE_ENV=development electron -r @babel/register .",
    "rebuild_electron": "electron-rebuild",
    "build_electron": "babel --out-dir ./build/server/ src/server/*.js && babel --out-dir ./build/common/ src/common/*.js",
    "start": "electron build/server/",
    "build": "npm run build_react && npm run build_electron",
    "build_docs": "plantuml -progress -tpng ./docs/*.puml",
    "package": "electron-packager . cs-performance-environment --platform darwin --overwrite"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error",
      "no-case-declarations": 0,
      "prefer-const": "error"
    }
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-plugin-istanbul": "^5.2.0",
    "electron": "^1.8.8",
    "electron-packager": "^13.1.1",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^1.8.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
