{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@serialport/bindings": "^9.0.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.8",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/uuid": "^8.3.0",
    "axios": "^0.18.1",
    "bjorklund-js": "^0.1.2",
    "bootstrap": "^4.5.2",
    "easymidi": "^1.1.2",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "hammerjs": "^2.0.8",
    "inobounce": "^0.1.6",
    "lodash": "^4.17.20",
    "node-osc": "^3.0.1",
    "note-parser": "^2.0.1",
    "pex-color": "^1.1.1",
    "pixi.js": "^4.8.9",
    "pluralize": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hammerjs": "^1.0.1",
    "react-redux": "^7.2.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^4.0.0",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0",
    "supercollider-redux": "1.0.0-alpha.2",
    "supercollider-redux-sequencers": "^0.9.0",
    "typescript": "^4.0.5",
    "uuid": "^3.4.0"
  },
  "main": "build/server/index.js",
  "scripts": {
    "start_react": "react-scripts start",
    "build_react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start_electron": "TS_NODE_PROJECT=electron.tsconfig.json NODE_ENV=development electron -r ts-node/register -r tsconfig-paths/register ./src/main/index.js",
    "rebuild_electron": "electron-rebuild",
    "postinstall": "npm run dev_codesign",
    "dev_codesign": "codesign --force --deep --sign - node_modules/electron/dist/Electron.app",
    "build_electron": "mkdir -p build/main && cp src/main/sclang_init.sc build/main/ && tsc --build electron.tsconfig.json",
    "start": "electron -r tsconfig-paths/register build/main/",
    "build": "npm run build_react && npm run build_electron",
    "build_docs": "plantuml -progress -tpng ./docs/*.puml",
    "package": "electron-packager . cs-performance-environment --platform darwin --overwrite"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "@typescript-eslint/no-use-before-define": "error",
      "no-use-before-define": "off",
      "block-scoped-var": "error",
      "no-case-declarations": 0,
      "prefer-const": "error",
      "react/prop-types": 0,
      "@typescript-eslint/no-extra-semi": "off",
      "@typescript-eslint/no-empty-function": "off"
    }
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "electron": "^6.1.12",
    "electron-packager": "^13.1.1",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^1.11.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
