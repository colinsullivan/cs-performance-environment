{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "awakening-sequencers": "^0.8.8",
    "bootstrap": "^3.3.7",
    "easymidi": "0.0.4",
    "lodash": "^4.17.11",
    "node-osc": "^3.0.0",
    "note-parser": "^2.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-scripts": "1.0.14",
    "supercollider-redux": "0.0.94",
    "supercolliderjs": "^0.14.2"
  },
  "main": "src/start_electron.js",
  "scripts": {
    "start_react": "react-scripts start",
    "build_react": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start_electron": "NODE_ENV=development electron -r @babel/register .",
    "rebuild_electron": "electron-rebuild",
    "build_electron": "babel --out-dir ./build/ src/start_electron.js",
    "build": "npm run build_react && npm run build_electron"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": false,
      "jsx": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "electron": "^1.8.8",
    "electron-react-devtools": "^0.5.3",
    "electron-rebuild": "^1.8.2"
  },
  "homepage": "./"
}
