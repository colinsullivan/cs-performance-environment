{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "abletonlink-redux": "0.0.7",
    "awakening-sequencers": "^0.7.8",
    "bootstrap": "^3.3.7",
    "note-parser": "^2.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "supercolliderjs": "^0.14.2"
  },
  "main": "src/start_electron.js",
  "scripts": {
    "start_react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start_electron": "NODE_ENV=development electron -r babel-register .",
    "rebuild_electron": "electron-rebuild",
    "build_electron": "babel --out-dir ./build/ src/start_electron.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": false,
      "jsx": true
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "electron": "^1.7.10",
    "electron-rebuild": "^1.6.0"
  },
  "homepage": "./"
}
